{
  "name": "civil-server",
  "version": "1.0.7",
  "description": "an extensible server component that can be part of other civic tech projects",
  "main": "dist/index.js",
  "scripts": {
    "start": "node --max_old_space_size=460 dist/start.js",
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "dbtest": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand --config ./jest-db.config.js --runTestsByPath",
    "cypress": "cypress open",
    "cypress:headless": "cypress run --headless --browser chrome",
    "mongo:delete-user": "bash -c 'mongo --eval \"db.users.remove({email: \\\"$0\\\"})\"'",
    "svgr": "svgr --icon --filename-case kebab assets/svg --out-dir app/svgr",
    "transpile": "babel app --ignore **/__tests__ --out-dir dist --source-maps",
    "hot-transpile": "babel --watch app --ignore **/__tests__ --out-dir dist --source-maps",
    "hot-server": "nodemon --inspect dist/start.js",
    "hot-server-no-inspect": "nodemon dist/start.js",
    "hot-client": "webpack-dev-server --config webpack-dev.config.js --devtool source-map --host 0.0.0.0",
    "prepare": "bash build.sh",
    "packbuild": "webpack --config webpack-prod.config.js",
    "dev": "echo \"these delays need to be kept up to date\" && concurrently -k \"npm run hot-transpile\" \"sleep 20 && npm run hot-server\" \"sleep 15 && npm run hot-client\"",
    "dev-no-inspect": "echo \"these delays need to be kept up to date\" && concurrently -k \"npm run hot-transpile\" \"sleep 20 && npm run hot-server-no-inspect\" \"sleep 15 && npm run hot-client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EnCiv/civil-server.git"
  },
  "author": "David Fridley",
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/EnCiv/civil-server/issues"
  },
  "homepage": "https://github.com/EnCiv/civil-server#readme",
  "optionalDependencies": {
    "@shelf/jest-mongodb": "^4.3.2",
    "concurrently": "^6.4.0",
    "cypress": "^9.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "jest": "^29.7.0",
    "jest-enzyme": "^7.1.2",
    "mongodb-memory-server": "^9.3.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "webpack-dev-server": "^4.6.0"
  },
  "peerDependencies": {
    "civil-client": "github:EnCiv/civil-client"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/plugin-transform-react-inline-elements": "^7.16.0",
    "@babel/plugin-transform-regenerator": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "babel-loader": "^8.2.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "@enciv/mongo-collections": "0.0.3",
    "@hapi/joi": "^15.1.0",
    "@jest/globals": "^29.7.0",
    "@sap_oss/node-socketio-stream": "^1.0.3",
    "assert": "^2.0.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "classnames": "^2.3.1",
    "cloudinary": "^1.27.1",
    "compression": "^1.7.4",
    "constants-browserify": "^1.0.0",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.19.3",
    "device": "^0.3.12",
    "express": "^4.17.1",
    "express-rate-limit": "^5.5.1",
    "helmet": "^6.2.0",
    "is-email": "^1.0.2",
    "lodash": "^4.17.21",
    "log4js": "git+https://github.com/ddfridley/log4js-node.git#onbrowser",
    "log4js-extend": "^0.2.1",
    "marked": "^4.0.7",
    "mongodb": "^5.9.2",
